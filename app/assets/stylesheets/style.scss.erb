#icr_checkbox{
	/* Common styles */
	height: 22px;

.toggle {
  position: absolute;
  margin-left: -9999px;
  visibility: hidden;
}

.toggle + label {
  display: block;
  position: relative;
  cursor: pointer;
  outline: none;
  user-select: none;
}

[type="radio"][class*="toggle"]:checked + label {
  cursor: default;
}

/* Round flat */

  margin-top: 20px;
  margin-left: 21px;

  p {
    float: left;
    display: inline;
    margin:0px;
    font-size: 11px;
    color: white;
    padding-top: 5px;
    line-height: 13px;
    font-family: roboto;
  }

  .toggle-round-flat {  float:left; }
.toggle-round-flat + label {
  float: left;
  padding: 2px;
  width: 45px;
  height: 23px;
  background-color: #dddddd;
  border-radius: 23px;
  transition: background 0.4s;
}
.toggle-round-flat + label:before,
.toggle-round-flat + label:after {
  display: block;
  position: absolute;
  content: "";
}
.toggle-round-flat + label:before {
  top: 2px;
  left: 2px;
  bottom: 2px;
  right: 2px;
  background-color: $widget_background;
  border-radius: 23px;
  transition: background 0.4s;
}
.toggle-round-flat + label:after {
  top: 4px;
  left: 4px;
  bottom: 4px;
  width: 16px;
  background-color: #dddddd;
  border-radius: 16px;
  transition: margin 0.4s, background 0.4s;
}
.toggle-round-flat:checked + label {
  background-color: #8ce196;
}
.toggle-round-flat:checked + label:after {
  float: left;
  margin-left: 21px;
  background-color: #8ce196;
}

/* Disabled state */
.toggle-round-flat:disabled + label {
  float: left;
  cursor: default;
  opacity: 0.5;
  
  &:before {
    background-color: #f3f3f3;
  }
}
}

/* Disable select img when dragged */
img {
-webkit-user-select: none;
-khtml-user-select: none;
-moz-user-select: none;
-o-user-select: none;
user-select: none;
}

