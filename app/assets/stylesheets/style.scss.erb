#dialing, #search_dialing{
  position:absolute;
  left:0;
  right: 0;
  bottom: 0;
  top:0;
  z-index: 6001;

  width: 100%;
  height: 100%;
  background: #171717; /* Old browsers */
  //background: -moz-linear-gradient(-45deg,  #cfd8dc 0%, #607d8b 100%, #b0bec5 100%); /* FF3.6-15 */
  //background: -webkit-linear-gradient(-45deg,  #cfd8dc 0%,#607d8b 100%,#b0bec5 100%); /* Chrome10-25,Safari5.1-6 */
  //background: linear-gradient(135deg,  #cfd8dc 0%,#607d8b 100%,#b0bec5 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#cfd8dc', endColorstr='#b0bec5',GradientType=1 ); /* IE6-9 fallback on horizontal gradient */
 
  p {
    padding-top: 38%;
    width: 100%;
    color: white;
    opacity: .9;
    font-size: 38px;
    text-align: center;
  }
  p#room{
    margin-top: -40px;
    font-size: 28px;  
    display: none;
  }
  .container {
    width: 200px;
    height: 200px;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    filter: url('#goo');
    animation: rotate-move 2s ease-in-out infinite;
  }

  .dot { 
    width: 58px;
    height: 58px;
    border-radius: 50%;
    background-color: #000;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
  }

  .dot-3 {
    background-color: #428bca;
    //background-color: #0085c4;
    animation: dot-3-move 2s ease infinite;
  }

  .dot-2 {
    background-color: #428bca;
    //background-color: #b4b4b4;
    animation: dot-2-move 2s ease infinite;
  }

 .dot-1 {
    background-color: #428bca;
    animation: dot-1-move 2s ease infinite;
  }

  @keyframes dot-3-move {
    20% {transform: scale(1)}
    45% {transform: translateY(-9px) scale(.45)}
    60% {transform: translateY(-45px) scale(.45)}
    80% {transform: translateY(-45px) scale(.45)}
    100% {transform: translateY(0px) scale(1)}
  }

  @keyframes dot-2-move {
    20% {transform: scale(1)}
    45% {transform: translate(-8px, 6px) scale(.45)}
    60% {transform: translate(-40px, 30px) scale(.45)}
    80% {transform: translate(-40px, 30px) scale(.45)}
    100% {transform: translateY(0px) scale(1)}
  }

  @keyframes dot-1-move {
    20% {transform: scale(1)}
    45% {transform: translate(8px, 6px) scale(.45)}
    60% {transform: translate(40px, 30px) scale(.45)}
    80% {transform: translate(40px, 30px) scale(.45)}
    100% {transform: translateY(0px) scale(1)}
  }

  @keyframes rotate-move {
    55% {transform: rotate(0deg)}
    80% {transform: rotate(720deg)}
    100% {transform: rotate(720deg)}
  }
}



