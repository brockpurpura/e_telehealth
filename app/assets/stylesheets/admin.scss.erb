      clear: both;
    }
    .bar:nth-of-type(2n) {
     // border: 1px solid black;
    }
    .bar .label {
      -webkit-transform: rotate(180deg);
      -moz-transform: rotate(180deg);
      -o-transform: rotate(180deg);
      -ms-transform: rotate(180deg);
      transform: rotate(180deg);
      margin-top: -2px;
      font-size: 8px;
      padding: 1em;
      background: #595959;
      width: 58px;
      color: white;
      display: inline-block;
      position: relative;
      z-index: 2;
      font-weight: bold;
      font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
       -webkit-font-smoothing: antialiased;    
    }
    .bar .label.warning {
      background: gold;
      width: 57px;
      margin-left: -1px;;
    }
    .count {
      display: none;
      position: absolute;
      -webkit-transform: rotate(180deg);
      -moz-transform: rotate(180deg);
      -o-transform: rotate(180deg);
      -ms-transform: rotate(180deg);
      transform: rotate(180deg);
      color: #fff;
      left: 1.6em;
      bottom: .75em;    
      padding: .15em;
      font-size: .75em;
      font-family:"Montserrat", sans-serif;
    }
  }
 
  .flash_button {
    border-bottom-left-radius: 60px;
    border-bottom-right-radius: 60px;
    border-top-left-radius: 60px;
    border-top-right-radius: 60px;
    height: 15px;
    width: 15px;
    margin: auto;

    &.for_edit {
      width: 12px;
      height: 12px;
      margin-left: 35px;
      margin-top: -40px;
      margin-bottom: 15px;    }
    &.warning {
      background-color: red;
      -webkit-animation: flash_red 2s infinite;
      animation: flash_red 2s infinite;
     }
    &.caution {
      background-color: gold;
      -webkit-animation: flash_yellow 2s infinite;
      animation: flash_yellow 2s infinite;
     }
     &.healthy {
      background-color: lime;
      -webkit-animation: flash_green 2s infinite;
      animation: flash_green 2s infinite;
     }
     &.not_configured {
       -webkit-animation: flash_grey 2s infinite;
       animation: flash_grey 2s infinite;
       background-color: #aaa2a2;
     }
  }
   
  @-webkit-keyframes flash_red {
    0% { background-color: red; }
    50% { background-color: white; }
  }
  @keyframes flash_red {
    0% { background-color: red; }
    50% { background-color: white; }
  }
  @-webkit-keyframes flash_yellow {
    0% { background-color: gold; }
    50% { background-color: gold; }
  }
  @keyframes flash_yellow {
    0% { background-color: gold; }
    50% { background-color: gold; }
  }
  @-webkit-keyframes flash_green {
    0% { background-color: lime; }
  }
  @keyframes flash_green {
    0% { background-color: lime; }
  }
    @-webkit-keyframes flash_grey {
    0% { background-color: #aaa2a2; }
  }
  @keyframes flash_grey {
    0% { background-color: #aaa2a2; }
  }


