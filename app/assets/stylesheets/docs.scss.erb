.docs{

  background-color:rgba(120, 144, 156, 1);
  height: 100%;
  div { float: left;
  }

  span {
    float: left;
    margin-bottom: 10px;
    margin-top: 10px;
    width: 100%;
  }

  h1, h2, h3, h4, h5, h6 {
    font-family: "Roboto";
    line-height: 1.3;
  }

  h1 {
    color: #404040;
    font-weight: 300;
    font-size: 1.75em;
    margin-top: 19px;
  }

  h2 {
    color: #fff;
    font-size: 35px;
    font-weight: 400;
    text-align: center;
  }

  h3 {
    color: #fff;
    font-size: 20px;
    font-weight: 400;
    text-align: center;
  }

  h4 {
    color: #fff;
    font-size: 16px;
    font-weight: 400;
    text-align: left;
    white-space: pre;
    margin-top: 34px;
    &.breadcrumbs {
      margin-top: 10px;
    }
  }
  

  h5 {
    color: #404040;
    margin-top: 32px;
    font-weight: 350;
  }

  p {
    font-size: 13px;
    color: #404040;
    font-weight: 300;
    margin-top: 14px;
    margin-bottom: 14px;
  }

  .docs_header {
    width: 100%;
    background-color: rgba(120, 144, 156, 0.7);
    border: 1px solid gray; 
    padding: 0;
  }
 
  .container {
    width: 100%;
    padding:0px;
  }

  .docs_container {
    width: 100%;
    background-color: rgb(66, 139, 202);
  }

  .docs_nav {
    float: left;
    background-color: rgba(120, 144, 156, 0.7);
    line-height: 2em;
    padding:0px;
    a, a:link, a:visited {
      float:left;
      margin-bottom: 8px;
      width: 100%;
      padding: 10px;
    }
  }

  pre {
    padding: 7px;
  }

  .docs_text {
    background-color: #d8e7f3;
    padding-right: 15;
    line-height: 1em;
    min-height: 1000px;
    a, a:hover, a:visited {
      color: rgb(66, 139, 202);
    }
    h4 {
      color: #404040;
      font-size: 15px;
    }
  }

  .docs_code {
    background-color: white;
    font-family: monaco;
    min-height: 1000px;
    padding-top: 15px;
    margin-top: 0px;
  }
  
  .docs_language {
    float: right;
    background-color: rgba(120, 144, 156, 0.7);    
  }

  .docs_table {
    padding-left: 15px;
  }

  ul {
    margin-left: 42px;
    margin-top:20px;
    li {
      font-size: 13px;
      color: #404040;
      font-weight: 300;
      margin-bottom: 15px;
    }
  }

  .d_body{
    height: 100%;
   }

  p.bheader {
    color: white;
    border-top: 1px solid white;
    border-bottom: 1px solid white;    
    background-color: rgba(255,255,255,.1); 
    float:left;
    padding: 10px;
    width:100%;
  }
}  //end of docs
