  }
}


#privacy_logo {
  width: 250px;
}


#trust_logo {
  margin-right: -15px;
}


.priv_logo {
  p { 
    display: inline-block;
    color: darkgray;
    font-size: 12px;
  }
  a {
    color: darkgray; 
    &.privacy_policy {
      margin-left: 10px;
    }
  }
  width: 264px;
  margin: auto;
}  

/* Header Bar */ 

.header_control_bar {
  position: fixed;
  top: 0;
  height: 54px;
  background-color: #4b4b4b; 
  opacity: 1;
  right: 0;
  left: 0;
  z-index: 200000;

  #search_box_view {
    display: inline !important;
    button{
      height: 54px;
      background: none;
      font-family: 'Roboto';
      font-size: 13px;
      color: #C9C9C9;
      padding: 0px 5px 0px 0px;
      margin: 0px 0px 0px 6px;
      &.no_notifications {
        margin: 0px 0px 0px 56px;
      }
      .fa.fa-comment {
        margin-left: 10px;
        font-size: 25px;
        color: #317ab9;
        opacity: 1.0;
        margin-top: -2px;
        -webkit-font-smoothing: unset;
        &:hover {
          color: #317ab9;
          text-decoration: none;
        }
      }
      p {
        font-size: 13px;
        margin-bottom: 0px;
        color: #C9C9C9;
        float: right;
        margin-left: 10px;
        &.parts {
          float: left;
        }
      }
     
      #chat_js_count {
        width: 25px;
        border-radius: 20px;
        opacity: 1;
        color: white;
        text-decoration: none;
        position: absolute;
        float: left;
        margin: 0;
        top: 16px;
        z-index: 100000000;
        display: block;
        &hover {
          text-decoration: none;
        }
        &.on_bounce {
          top: 2px;
        }
      }
    }
    &:hover{
      p {
        text-decoration: underline;
        color: white;
      }
    }
  }

  span {
    margin-left: 5px;
  }

  .fa.fa-sign-out, .fa.fa-user.li, .fa.fa-info-circle.li, .fas.fa-keyboard.li, .fas.fa-video.li {
    color: black;
    width: 15px;
  } 

  #header_left{
    width: 300px;
    height: 54px;
    float: left;
    #notif_header {
      button {
        display: inline;
        height: 54px;
        background: none;
        margin: 0;
        text-decoration: none;
        padding: 0px 5px 0px 0px;
        margin: 0px 0px 0px 56px;
        p {
          font-size: 13px;
          margin-bottom: 0px;
          color: #C9C9C9;
          float: left;
          margin-left: 10px;
          &.notifs {
            float: left;
          }
        }
        &.close_widget {
          height: 22px;
          position: absolute; 
          color: white;
          font-size: 22px;
          float: right;
          top: 14px;
          right:15px;
          opacity: 0.7;
          background: none;
          padding: 0px !important;
          &:focus {
            background: none;
          }
          &:hover {
            background: none;
          }
        }
        #js_count {
          width: 20px;
          height: 20px;
          border-radius: 20px;
          text-align: center;
          background-color: #317ab9;
          font-size: 12px;
          padding-top: 1px;
          margin-top: -1px;
          opacity: 1;
          color: white;
          text-decoration: none;;
          &hover {
            text-decoration: none;
          }
        }
        &:hover {
          opacity: 1.0;
          p {
            text-decoration: underline;
            color: white;
          }
        }
      }
    }
  }
 
  #right_of_header {
    width: 300px;
    float: right;
  
    #menu_button {
      background: none;
      height: 54px;
      padding: 0px 0px 0px 10px;
      margin: 0px 56px 0px 0px;
      display: flex;
      align-items: center;
      float: right;
      overflow-y: hidden;
 
      &:hover{
        color: white;
        .caret, .fa-user.dropdown {  
          color: white;
        }
      }

     .fa-user.dropdown {
        float: left;
        margin: -1px 10px 0px 0px;
        font-size: 13px;
        line-height: 13px;
        color: #C9C9C9;
        opacity: 1.0 !important; 
        &.api_username {
          margin-right: 0px;
        }
      }

      #top_dr_name {
        margin-top: 1px;
        margin-left: 0px;
      }
     .caret {
       margin-left: 10px;
       color: #C9C9C9;
       margin-right: 10px;
      }
     
    }

    .dropdown-menu {
      top: 52px;
      z-index: 20003;
      position: absolute;
      right: 50px;
      display: none;
      min-width: 130px;
      padding: 5px 0;
      margin: 2px 0 0;
      list-style: none;
      font-size: 14px;
      text-align: left;
      background-color: #fff;
      left: unset;
      border: 1px solid rgba(0, 0, 0, 0.15);
      border-radius: 4px;
      box-shadow: 0 6px 12px rgba(0, 0, 0, 0.175);
      background-clip: padding-box
    }
    .open > .dropdown-menu {
      display: block;
    }
    .dropdown-menu > li > a {
      display: block;
      padding: 3px 22px;
      &.favorite_star {
        padding-left: 21px;
      }
      &:focus {
        outline-offset: 1px;
      }
    }
  }
  
  a {
    color: #C9C9C9;  
    &:hover, &:focus {
      color: #fff;  
      p {
        text-decoration: underline;
      }
    }
  }

  p, h1 {
    font-size: 13px;
    margin-bottom: 0px;
    color: #C9C9C9;
    float: left;
    display: inline-block;
    &.r_name {
      width: calc(100% - 60px);
      height: 54px;
      display: flex;
      align-items: center;
      margin: 0px 15px;
      pointer-events: none;
      overflow-y: hidden;
      min-width: 200px;
      max-width: 600px;
      justify-content: center;
    }
    &.settings {
      margin-left: 20px;
      margin-right: 20px;
      &:hover {
        opacity: 1.0;
      }
    }
  }
}


/*  FA Icons  */

.fa, .far{
  color: white;
  opacity: .7; 
  &:hover {
    color: white;
    opacity: 1; 
  }
}

.fas {
  color: white;
  opacity: .7; 
  &:hover {
    color: white;
    opacity: 1; 
  }
}


#room_time {
  background-color: transparent;
  position: relative;
  text-align: center;
  color: white;
  width: calc(100% - 600px);
  margin: 0px auto;
  float: left;
  height: 54px;
  display: flex;
  align-items: center;
  
  .room_wrapper {
    text-align: center;
    margin: auto;
  }
  
  /* Toggle / Lock Features */

  #favorite_toggle {
    align-items: center;
    opacity: 1;
    display: flex;
    height: 54px;
    float: left;
    width: 15px;
    
    button {
      background: transparent; 
      padding: 0; 
      margin: 0;
      font-size: 14px;
      .fas.fa-star {
        color: white; 
      }
    }
  }
  #lock_vmr_toggle {
    align-items: center;
    opacity: 1;
    display: flex;
    height: 54px;
    float: left;
    width: 15px;

    button {
      background: transparent; 
      padding: 0; 
      margin: 0; 
      font-size: 13px;
      .fas.fa-lock{
        color: white; 
      }
    }

    .deactivated {
      cursor: not-allowed;
    }
  }

  .tool-tip-icon {
    &:hover {
      .callout {
        visibility: visible;
        opacity: 1;
      }
    }
    .callout {
      width: 83px;
      margin: 32px 0px 0px -32px;
      background-color: rgba(0,0,0,0.6);
      position: absolute;
      color: white;
      padding: 3px;
      border-top-left-radius: 5px;
      border-top-right-radius: 5px;
      border-bottom-left-radius: 5px;
      border-bottom-right-radius: 5px;
      opacity: 0;
      top: 16px;
      pointer-events: none;
