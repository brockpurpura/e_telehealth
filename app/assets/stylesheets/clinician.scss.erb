    height: 200px;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    filter: url('#goo');
    animation: rotate-move 2s ease-in-out infinite;
  }

  .dot { 
    width: 58px;
    height: 58px;
    border-radius: 50%;
    background-color: #000;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
  }

  .dot-3 {
    background-color: #428bca;
    //background-color: #0085c4;
    animation: dot-3-move 2s ease infinite;
  }

  .dot-2 {
    background-color: #428bca;
    //background-color: #b4b4b4;
    animation: dot-2-move 2s ease infinite;
  }

 .dot-1 {
    background-color: #428bca;
    animation: dot-1-move 2s ease infinite;
  }

  @keyframes dot-3-move {
    20% {transform: scale(1)}
    45% {transform: translateY(-9px) scale(.45)}
    60% {transform: translateY(-45px) scale(.45)}
    80% {transform: translateY(-45px) scale(.45)}
    100% {transform: translateY(0px) scale(1)}
  }

  @keyframes dot-2-move {
    20% {transform: scale(1)}
    45% {transform: translate(-8px, 6px) scale(.45)}
    60% {transform: translate(-40px, 30px) scale(.45)}
    80% {transform: translate(-40px, 30px) scale(.45)}
    100% {transform: translateY(0px) scale(1)}
  }

  @keyframes dot-1-move {
    20% {transform: scale(1)}
    45% {transform: translate(8px, 6px) scale(.45)}
    60% {transform: translate(40px, 30px) scale(.45)}
    80% {transform: translate(40px, 30px) scale(.45)}
    100% {transform: translateY(0px) scale(1)}
  }

  @keyframes rotate-move {
    55% {transform: rotate(0deg)}
    80% {transform: rotate(720deg)}
    100% {transform: rotate(720deg)}
  }
}

