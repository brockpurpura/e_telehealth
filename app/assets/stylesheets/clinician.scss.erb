/*  This file will contain all login items incase we do not separate them into their own stylesheets  */
$outline-color: #317ab9;

@font-face {
  font-family: "FontAwesome";
  src: font_url('fontawesome-webfont.eot');
  src: font_url('fontawesome-webfont.eot?#iefix') format('eot'), font_url('fontawesome-webfont.woff') format('woff'), font_url('fontawesome-webfont.ttf') format('truetype'), font_url('fontawesome-webfont.svg') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: "Roboto";
  src: font-url( asset-path("Roboto-Light.ttf"));
}

body{
  margin: 0;
  font-family: "Roboto";
  font-size: 13px;
  font-weight: normal;
  line-height: 18px;
  color: #404040;
  height: 100%;
  background-color: white;
}

video::-webkit-media-controls {
  display:none !important;
}

#main_body {
  overflow: auto;
}

p, h1, h2, h3, h4, h5, select, input, a, a:link, button, .fa {
  font-family:'Roboto';
  color: white;
  &.fa, &.fal, &.fas {
   font-family: 'Font Awesome\ 5 Pro';
  }
  &.fa-2x { 
    font-size: 2em;
  }

  &:focus {
    outline: 2px solid $outline-color !important; 
    outline-offset: 4px;
  }
} 


body.using-mouse :focus {
  outline: none !important;
}

a:hover, a:focus {
  opacity: 1.0;
}

.support {
 .wrapper {
    padding: 100px 60px;
  }
  #sub_title {
    height: 30px;
  }
  h1 {  
    font-size: 20px;
    margin: 2px 0px 0px 15px;
    color: #212121;
    line-height: 23px;
    font-weight: 600px;
  }
  h3 {
    font-size: 15px;
    font-weight: bold;
    padding: 2px 0px;
    margin: 86px 2px 0px 2px;
    color: #212121;
    line-height: 23px;
  }

  p {
    margin: auto;
    text-align: center;
    font-size: 13px;
    color: #212121;
    margin-top: 20px;
    font-family: 'Roboto';
    .back_header {
      margin-left: 60px;
    }
    &.sections {
      font-size: 13px;
      color: black;
      display: block;
      margin: 0px 0px 20px 0px;
      font-weight: 700;
      font-family: 'helvetica';
      padding: 0px;
      text-align: left;
    }
  } 
 
  hr {
    border-bottom: 1px solid #cccccc;
    margin: 18px 0px 45px 0px;
    padding: 0px;
    border-top: 0px;
  }

  .tutorial_wrapper {
    float: left;
    width: 100%;
    margin-bottom: 45px; 
  }

  .iframe {
    margin-right: 45px;
    height: 225px;
    margin-bottom: 90px;
  }

  i.fa {
    color: #212121;
    opacity: 1.0;
  }
  
  a {
    color: #336699;
    display: block;
    margin-bottom: 20px;
    &.support_back {
      margin-left: 60px; 
     margin-top: 20px;
      .fa {
        color: white; 
      }
      p {
        margin-top: 0px;
      }
    }
  }
}

.blue_settings_link{
  display: inline-block;
  width: 100%;
}

.blue_sound_test, .blue_settings_link{
  background: none;
  padding: 4px;
  color: #007cc2 !important;
  font-size: 11px;
  &:focus, &:hover {
  color: #007cc2 !important;
  background: none;
  text-decoration: underline;
   }
}


button {
  &.form_button {

    box-shadow: inset 0 0 1px 1px rgba(255, 255, 255, 0);
    transition: all .2s ease;
    border-style: solid;
    border-width: 0;
    font-weight: 400;
    line-height: normal;
    position: relative;
    text-decoration: none;
    text-align: center;
    -webkit-appearance: none;
    -webkit-border-radius: 0;
    display: inline-block;
    font-size: 1rem;
    background: #007cc2;
    border: none;
    padding: 10px;
    border-radius: 3px;
    color: #fff;
    cursor: pointer;
    margin: auto;


    width: 184px;
    font-family: 'Roboto';
    font-size: 13px;
    &:focus {
      outline-offset: 4px;
    }  
  }
}

input[type=checkbox], input[type=radio] {
  &:focus {
    outline-offset: 4px;
    outline: 2px solid red;
  }
}


select, input[type=text] {
    -webkit-appearance: none;
    -moz-appearance: none;
    text-indent: 1px;
    text-overflow: '';
    width: 100%;
    height: 37px;
    padding: 0px 25px 0px 10px;
    margin-bottom: 23px;
    font-size: 11px;
    border-radius: 5px;
    border-style: solid;
    border-width: 1px;
    border-color: #bec9cf;
    background-color: white;
    color: #757575;

    &:focus {
      outline-offset: 4px;
    } 
    &.invalid {
      border-bottom: 2px solid red;
      color: red;
      option {
        color: #757575;
      }
    } 
    &.audio_output_select {
      margin-bottom: 10px;
    }
}

label {
    display: inline-block;
    max-width: 100%;
    margin-bottom: 7px;
    font-weight: bold;
    font-size: 13px;
    color: #757575;
}

#e_m { 
  color: darkgray;  
  margin-left: 40px;
}


.alert {
    margin-bottom: 0px;
}

.blocked {
  position: fixed;
  top: 0px;
  bottom:0;
  left:0;
  right: 0;
  background-color: black;
  opacity: .7;
  z-index: 20000;
}


/* Height / Arraingment */

.height_5 { margin-top: 5px; }
.height_10 { margin-top: 10px; }
.height_15 { margin-top: 15px; }
.height_25 { margin-top: 25px; }
.left_n2 { margin-left: -2px; }
.left_n4 { margin-left: -4px; }
.left_n9 { margin-left: -9px; }
.left_n10 { margin-left: -10px; }




.leftside {
  float: left;
}
.rightside {
  float: right;
}  
.center {
  margin: auto;	
  width: 100%;
  text-align:center; 
}


/* Dropdowns */

  .dropdowns {
    span {position:relative; width: 100%; margin-bottom: 0px; display: inline-block; float: right;}
     span:after {
      content:"";   
      color:#777;
      right:13px; 
      top:16px;
      position:absolute;
      border-top: solid 4px rgb(119,119,119);
      border-left: solid 4px transparent;
      border-right: solid 4px transparent;
      pointer-events: none;    
       
    }

    span:before {
      content:'';
      right:4px; 
      top:0px;
      width:23px;
      height:37px;
      position:absolute;
      pointer-events:none;
      display: block;
           
    }
  }

  /*  Buttons */
  .btn_login {
         background: linear-gradient(160deg, #428bca 0, #428bca 100%);
         background: -webkit-linear-gradient(160deg, #428bca 0, #428bca 100%);

	  
    background-color: #428bca;
    border: none;
    padding: 10px;
    width: 100%;
    border-radius: 3px;
    color: #fff;
    cursor: pointer;
    margin: auto;
    &.setting_login {
      width: 184px;
      font-family: 'Roboto';
      font-size: 13px;
      margin-left: 28px;
      &.change_speaker{
        margin-top: 20px;
        margin-bottom: 15px;
      }
      &.chat_view {
        margin-left: 0px;
      }
    }
    &.conf_login {
      width: 184px;
      margin: -10px 10px;
      font-family: 'Roboto';
      font-size: 13px;  
    }
  }


/* Settings Side Bar */ 

.call_settings_view_surround {
  background-color: transparent;
  position: absolute;
  top: 54.5px;
  right: 0;
  bottom: 0;
  z-index: 20002;
  height: calc(100vh - 55px);
  
  .call_settings_view {
    background-color: white;
    position: relative;
    width: 284px;
    height: 100%;
    .header_ {
      width: 100%;
      height: 36px;
      padding-left: 20px;
      padding-top: 7px;
      color: white;
      background: linear-gradient(135deg, #cfd8dc 0%, #607d8b 100%, #b0bec5 100%);
      text-shadow: 0 1px 1px darkgray;
    }
    
    label.more_margin {
      margin-top: 22px; 
    }

    p {
      font-weight: bold;
      font-size: 13px;
      color: #757575;
      margin: 2px 0px 15px 5px
    }
    h2 {
      color: #627a86;
      font-size: 20px;
      margin: 15px 0px;
      text-align: center;
    }
    .close_button_wrapper {
      padding: 24px;
      text-align: center;
    }
    .form_button {
      margin-bottom: 23px;
    }
  }
  .c_settings {
    width: 100%;
    margin: auto;
    color: #757575;
    font-size: 16px;
    background: white;
    padding:24px;
    height: 100%;
    overflow: auto;

    &.mobile {
      margin-top: 75px;
    }
    &.favorites {
      padding: 24px 10px 24px 24px;
    }

    #videoSettingView {
      width: 240px;
      margin: 10px 0px;
      border-radius: 5px;
    }

    .web_app_version {
      position: relative;
      text-align: center;
      
      p {
        color: #757575;
        font-size: 13px;
        margin: 0px 0px 25px 0px;
        width: 100%;
      }
    }
    
    label {
      margin-top: 5px;
      &.receive_notifications {
        font-weight: bold; 
        font-size: 13px; 
        color: #757575; 
        margin: 3px 0px 25px 5px;
      }
    }

    ul {
      margin: 0; 
      padding-right: 10px;
      position: absolute; 
      top: 85px; 
      bottom: 70px; 
      width: 246px; 
      overflow-y: scroll;
     
      li {
        float: left; 
        display: flex;
        border-bottom: 1px solid #bec9cf; 
        width: 100%; 
        align-items: center;
        min-height: 55px;
        
        p {
          float: left; 
          display: flex; 
          width: 100%; 
          margin: 10px 0px;
        }
  
        button {
          width: 21px; 
          height: 21px; 
          padding: 0px; 
          margin: 0px 15px 0px 0px; 
          background: transparent; 
         
          .fas.fa-minus-circle {
            color: red; 
            font-size: 21px;
          }
          .fa.fa-phone {
            color: #757575; 
            font-size: 21px;
            margin-left: 10px;
          }
        }
      }
    }
  }
}

.slider_button {
  position: absolute;
  bottom: 17px;
  width: 20px;
  height: 20px;
  
  &.right {
    right: 24px;
  }
  
  button {
    background: transparent;
    padding: 0px;
    margin-bottom: 0px;
    .far {
      color: #757575;
      font-size: 24px;
    }
    .fas {
      color: #757575;
      font-size: 20px;
    }
  }
}

/* Login Widget */

#login_widget_surround {
  background-color: transparent;
  position: absolute;
  top: 25vh;
  left: 50%;
  z-index: 500000;
  margin-left: -160px;
  &.favorite_rooms {
    top: 20vh;
  }

  #login_widget {
    background-color: white;
    border-radius: 15px;
    width: 320px;
    height: 535px;
    &.favorite_rooms {
      height: 566px;
    }  
  }

  .favorite_rooms_link {
    color: #007cc2; 
    font-size: 11px;
    margin: 23px 0px 24px 0px;
    display: block;
    border: 0;
    padding: 0;
    height: unset;
  }

  .call_privacy {
    display: inline-block;
    height: 15px;
    margin: 0px 0px 22px 13px;
  }

  .user_login {
    width: 242px;
    margin: auto;
    padding: 1px;

    .callout {
      left: -25px;
      position: absolute;
      color: white;
      padding: 7px 10px;
      border-top-left-radius: 5px;
      border-top-right-radius: 10px;
      border-bottom-left-radius: 5px;
      border-bottom-right-radius: 10px;
      opacity: 1;
      margin-top: 3px;
      pointer-events: none;
      background: #db3a0e;
    }

    .callout::before {
      content: "";
      width: 0px;
      height: 0px;
      border: 0.8em solid transparent;
      position: absolute;
      right: -18px;
      top: 50%;
      transform: translateY(-50%);
      border-left: 9px solid rgba(221, 60, 16, 1.0);
      display: block;
    }
  }

  h1, h2 {
    margin-top: 23px;
    margin-bottom: 25px;
    margin-left: auto;
    margin-right: auto;
    text-align: center;
    color: #627a86;
    font-size: 20px;
    line-height: 35px;
    font-weight: 600;
  }

  input {
    width: 100%;
    height: 37px;
    padding-left: 10px;
    margin-bottom: 23px;
    font-size: 11px;
    border-radius: 5px;
    border-style: solid;
    border-width: 1px;
    border-color: #bec9cf;
    background-color: white;
    color: #757575;
    &.checkbox {
      width: 12px;
      float: left;
      height: 12px;
      margin: 4px 7px 0px 0px;
      min-height: 12px;
      min-width: 12px;
    }
    &:focus {
      outline-offset: 4px;
    }
  }

  input[type=checkbox] {
    &:focus{
      outline: 5px solid -webkit-focus-ring-color;
      outline-offset: 4px;
    }
  }
  

  .checkbox_label {
    font-size: 11px;
    color: #757575;
    margin: 0px;
    height: 18px;
    padding: 1px 0px 0px 0px;
  }
  .buttons {
    padding-left:5px;
  }
}


a.user_forgot, a:hover.user_forgot {
  color: blue;
}


#settings_link {
  margin-top: 27px;
  padding-top: 0px;
  margin-left: 102px;
  a {
    &.settings_link {
      color: #428bca;
      font-size: 11px;
      &:focus {
        text-decoration: underline;
      }
