.d_and_d {
  .widget {
    float:left;
    width: 100%;
    color: #777777;
    min-height: 500px;
    p { 
     color: #757575;
     font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
     -webkit-font-smoothing: antialiased;
     font-size: 13px;
      margin: -3px 0px 0px 0px !important;
    }
    .widget-content {
      padding: 20px;
      width: 550px;
    }
  }
  .permissions_widget {
    position: fixed;
    width: 250px;
    min-height: 250px;
    max-height: 250px;
    overflow-x: hidden;
    overflow-y: auto;
    
  }
  width: 100%;
  float:left;
  .u_list {
    width: 60%;
    float:left;
    padding: 20px;
  }
  .c_list {
    width: 40%;
    float:left;
    padding: 10px;
  }

  p.ui-sortable-handle {
    color: #777777;
    font-size: 15px;
  }

  .ui-droppable-active { 
	  //background-color: gold;
	  background-color: #f6f6f6;
	  
	  opacity: 0.7;	  
  }

  .ui-state-hover {
    p, p.ui-sortable-handle {
    color: black;
    background-color: blue;
    }
  }

  .ui-droppable-hover {
	  //background-color: lime;
	  background-color: #ededed;
	  opacity: 0.7;
  }

  .fa-user-md, .fa-wrench, .fa-folder-o, .fa-folder-open-o {
    color: #757575;
    font-size: 11px;
  }

  .fa-trash {
    margin-left: 4px;
    color: #757575;
    font-weight: 100;
    font-size: 11px;
  }
  a,a:hover {
    color: white;
    text-decoration: none;
  }

  .client_h {
    color: #777777;
  }
  p.top_level {
  }
}
 
  ol {
    margin: 0px 0px 0px 0px;
  }
    ol li ,.draggable {
      list-style: none; 
      color: #757575;
      font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
      -webkit-font-smoothing: antialiased;
      font-size: 13px;
      z-index: 1000;
    }

   .bot_m {
     margin-bottom: 12px;
   } 
